        <!-- 可选 -->
        <activity android:name="com.pgyersdk.activity.FeedbackActivity" />

        <!-- 必选 -->
        <meta-data
            android:name="PGYER_APPID"
            android:value="e259888bef73da591d609eae067aeaef" >
        </meta-data>
    </application>

</manifest>